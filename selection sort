x = int(input("enter num of elem: "))
l=[]
for i in range(x):
  b=int(input("enter elements: "))
  a = l.append(b)
i = 0
while i<len(a):
    #smallest element in the sublist
    smallest = min(a[i:])
    #index of smallest element
    index_of_smallest = a.index(smallest)
    #swapping
    a[i],a[index_of_smallest] = a[index_of_smallest],a[i]
    i=i+1
print (a)
